generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ChargeBoxNotification {
  notificationPk           Int      @id @map("notificationPk")
  type                     String
  chargeBoxId              String
  chargeBoxUuid            String?
  addressState             String? // null => opcional
  addressCity              String?
  addressStreet            String?
  registrationStatus       String?
  connectorErrorCode       String?
  connectorErrorMsg        String?
  chargeBoxName            String
  notificationTimestampDT  String @map("notificationTimestampDT")
  userName                 String?
  lastName                 String?
  idTag                    String?
  connectorType            String?
  userExternalId           String?
  chargeBoxGroupExternalId String?
  connectorId              Int?
  transactionId            Int?
  userPk                   Int?
  notified                 Boolean?

  @@map("chargeBoxNotifications")
}
